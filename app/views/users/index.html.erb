<p id="notice"><%= notice %></p>

<h1>User Information</h1>

<table>
  <thead>
  <tr>
    <th>Nickname</th>
    <th>Email</th>
    <th>Sign_in_count</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.nickname %></td>
        <td><%= user.email %></td>
        <td><%= user.current_sign_in_at %></td>
        <td><%= link_to 'Show', user_path(user.nickname) %></td>
        <td><%= link_to 'Edit', edit_user_registration_path(user) %></td>
      </tr>
  <% end %>
  </tbody>
</table>
